---
layout: none
---
[
  {% for post in site.posts %}
    {
      "title": {{ post.title | jsonify }},
      "url": "{{ post.url }}",
      "date": "{{ post.date | date: '%a. %b %d, %y' }}",
      "excerpt": {{ post.excerpt | strip_html | truncatewords: 50 | jsonify }},
      "categories": {{ post.categories | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
